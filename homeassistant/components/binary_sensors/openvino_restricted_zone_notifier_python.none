# https://www.home-assistant.io/integrations/binary_sensor.mqtt/

mqtt:
  binary_sensor:
    - name: OpenVINO Restricted Zone Notifier Python
      state_topic: "Restricted_zone_python"
      value_template: '{{ value_json.Alert }}'
      payload_on: True
      payload_off: False
      off_delay: 1
      device_class: motion
      force_update: true
